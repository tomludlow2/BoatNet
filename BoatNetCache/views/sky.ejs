<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Sky News Cached Articles</title>
    <style>
        body {
            font-family: Arial, sans-serif;
            text-align: center;
            padding: 20px;
        }
        h1 {
            color: #333;
        }
        ul {
            list-style-type: none;
            padding: 0;
        }
        li {
            padding: 10px;
        }
    </style>
</head>
<body>
    <h1>Sky News Cached Articles</h1>
    <ul>
        <% articles.forEach(article => { %>
            <li><a href="/bbc/<%= article.file %>"><%= article.title %></a></li>
        <% }) %>
    </ul>
    <a href="/">Back to Home</a>
</body>
</html>
